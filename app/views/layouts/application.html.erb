  </div>

  <div class="menu-2">

    <div class="bio">
      <h1>approximatif - shevi shenaj - hello</h1>
    </div>

    <div class="links">

      <div class="links1">
        <div class="photo-link">
          <%= link_to 'photo', photos_path %>
        </div>

        <div class="poster-link">
          <%= link_to 'poster', posters_path  %>
        </div>
      </div>

      <div class="links2">
        <div class="project-link">
          <%= link_to 'project', projects_path %>
        </div>

        <div class="shop-link">
          <%= link_to 'shop', shop_path %>
